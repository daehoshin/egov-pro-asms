<tiles-definitions>

	<definition name="mTemplate" template="/WEB-INF/views/mLayout.jsp">
		<put-attribute name="header" value="/WEB-INF/views/header.jsp" />
		<put-attribute name="left" value="/WEB-INF/views/left.jsp" />
		<put-attribute name="top" value="/WEB-INF/views/top.jsp" />
		<put-attribute name="section" value="" />
		<put-attribute name="footer" value="/WEB-INF/views/footer.jsp" />
	</definition>
  	
  	<definition name="/reg/manpower/ManPower" extends="mTemplate">
		<put-attribute name="section" value="/WEB-INF/jsp/asms/reg/manpower/ManPower.jsp" />
	</definition>
	
	<definition name="sTemplate" template="/WEB-INF/views/sLayout.jsp">
		<put-attribute name="section" value="" />
	</definition>
	
	<definition name="/reg/manpower/*" extends="sTemplate">
		<put-attribute name="section" value="/WEB-INF/jsp/asms/reg/manpower/{1}.jsp" />
	</definition> 
	
</tiles-definitions>