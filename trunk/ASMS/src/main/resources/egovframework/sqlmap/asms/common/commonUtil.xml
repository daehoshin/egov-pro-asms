	<select id='findComCdList' parameterClass="asms.common.code.service.CommonCodeVO" resultMap="codeinfo-detail">
			SELECT COM_CD AS CODE, 
					COM_CD_NM AS NAME
			FROM TB_ZZCOMCD
			<dynamic prepend="WHERE">
				<isNotEmpty prepend="AND" property="type">
					COM_TYPE_CD = #type:VARCHAR#
				</isNotEmpty>
				<isNotEmpty prepend="AND" property="use">
					USE_FLAG = #use:VARCHAR#
				</isNotEmpty>
				<isNotEmpty prepend="AND" property="code">
					COM_CD = #code:VARCHAR#
				</isNotEmpty>
				<isNotEmpty prepend="AND" property="name">
					COM_CD_NM = #name:VARCHAR#
				</isNotEmpty>
				<isNotEmpty property="exCode" prepend="AND">
					<![CDATA[
					 COM_CD
					]]>
					<iterate prepend="NOT IN" property="exCode" open="(" close=")" conjunction=",">
					 #exCode[]#
					</iterate>
					</isNotEmpty>
			</dynamic>
			ORDER BY COM_CD_RANK, DECODE(COM_CD_RANK, NULL, TO_NUMBER(COM_CD), TO_NUMBER(COM_CD_RANK))
	</select>
	
	<select id="menu_grant" parameterClass="asms.common.code.service.CommonCodeVO" resultMap="codeinfo-detail">
			SELECT MENU_GRP_ID AS CODE, MENU_GRP_NM AS NAME FROM TB_STGRPGRANT
			WHERE USE_FLAG = 0
			<dynamic>
			<isNotEmpty prepend="AND" property="code">
				MENU_GRP_ID = #code:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				MENU_GRP_NM = #name:VARCHAR#
			</isNotEmpty>
			</dynamic>
			ORDER BY MENU_GRP_ID
	</select>
	
